{
  "openapi": "3.0.0",
  "info": {
    "title": "Child Event Manager Database API",
    "version": "3.0.0",
    "description": "API for managing users, children, events, and bookings in the database"
  },
  "paths": {
    "/get-users": {
      "post": {
        "summary": "Get all users or filter by criteria",
        "description": "Retrieves user information from the database. Use this to find valid user IDs before creating children or bookings.",
        "operationId": "getUsers",
        "requestBody": {
          "required": false,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string",
                    "description": "Filter by exact email address"
                  },
                  "name": {
                    "type": "string",
                    "description": "Filter by user's name (partial match)"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "List of users",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "users": {
                      "type": "array",
                      "items": {
                        "type": "object"
                      }
                    },
                    "count": {
                      "type": "integer"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/get-children": {
      "post": {
        "summary": "Get all children or filter by criteria",
        "description": "Retrieves children information from the database. Can filter by user ID or child name.",
        "operationId": "getChildren",
        "requestBody": {
          "required": false,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "userId": {
                    "type": "string",
                    "description": "Filter by user ID"
                  },
                  "firstName": {
                    "type": "string",
                    "description": "Filter by child's first name"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "List of children",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "children": {
                      "type": "array",
                      "items": {
                        "type": "object"
                      }
                    },
                    "count": {
                      "type": "integer"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/get-child-details": {
      "post": {
        "summary": "Get detailed information about a specific child including their events",
        "description": "Retrieves comprehensive information about a child including all their events, age, and related data",
        "operationId": "getChildDetails",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "childId"
                ],
                "properties": {
                  "childId": {
                    "type": "string",
                    "description": "The ID of the child"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Detailed child information with events",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "child": {
                      "type": "object"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/create-child": {
      "post": {
        "summary": "Create a new child record",
        "description": "Adds a new child to the database. IMPORTANT: First call without confirmed to get a preview, then call again with confirmed=true after user confirms. Always pass both userId and userEmail from the system context.",
        "operationId": "createChild",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "firstName",
                  "dateOfBirth"
                ],
                "properties": {
                  "confirmed": {
                    "type": "boolean",
                    "description": "Set to true only after user confirms"
                  },
                  "firstName": {
                    "type": "string",
                    "description": "Child's first name"
                  },
                  "lastName": {
                    "type": "string",
                    "description": "Child's last name"
                  },
                  "dateOfBirth": {
                    "type": "string",
                    "description": "Date of birth in YYYY-MM-DD format"
                  },
                  "gender": {
                    "type": "string",
                    "description": "Child's gender"
                  },
                  "allergies": {
                    "type": "string",
                    "description": "Any allergies"
                  },
                  "medicalInfo": {
                    "type": "string",
                    "description": "Medical information"
                  },
                  "notes": {
                    "type": "string",
                    "description": "Additional notes"
                  },
                  "userId": {
                    "type": "string",
                    "description": "ID of the user from system context"
                  },
                  "userEmail": {
                    "type": "string",
                    "description": "Email of the user from system context (used as fallback)"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Child created or preview returned",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "child": {
                      "type": "object"
                    },
                    "message": {
                      "type": "string"
                    },
                    "requiresConfirmation": {
                      "type": "boolean"
                    },
                    "preview": {
                      "type": "object"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/update-child": {
      "post": {
        "summary": "Update child information",
        "description": "Updates an existing child's information. First call without confirmed to get a preview.",
        "operationId": "updateChild",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "childId"
                ],
                "properties": {
                  "confirmed": {
                    "type": "boolean",
                    "description": "Set to true only after user confirms"
                  },
                  "childId": {
                    "type": "string",
                    "description": "ID of the child to update"
                  },
                  "firstName": {
                    "type": "string"
                  },
                  "lastName": {
                    "type": "string"
                  },
                  "dateOfBirth": {
                    "type": "string"
                  },
                  "gender": {
                    "type": "string"
                  },
                  "allergies": {
                    "type": "string"
                  },
                  "medicalInfo": {
                    "type": "string"
                  },
                  "notes": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Child updated or preview returned",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    },
    "/delete-child": {
      "post": {
        "summary": "Delete a child record",
        "description": "Permanently deletes a child and all associated events and bookings. First call without confirmed to get a preview.",
        "operationId": "deleteChild",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "childId"
                ],
                "properties": {
                  "confirmed": {
                    "type": "boolean",
                    "description": "Set to true only after user confirms"
                  },
                  "childId": {
                    "type": "string",
                    "description": "ID of the child to delete"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Child deleted or preview returned",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    },
    "/create-event": {
      "post": {
        "summary": "Create a new event for a child",
        "description": "Logs a new event for a child. First call without confirmed to get a preview.",
        "operationId": "createEvent",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "childId",
                  "name",
                  "eventType"
                ],
                "properties": {
                  "confirmed": {
                    "type": "boolean",
                    "description": "Set to true only after user confirms"
                  },
                  "childId": {
                    "type": "string",
                    "description": "ID of the child"
                  },
                  "name": {
                    "type": "string",
                    "description": "Event name/description"
                  },
                  "eventType": {
                    "type": "string",
                    "description": "Type of event (medical, milestone, school, activity)"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Event created or preview returned",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    },
    "/delete-event": {
      "post": {
        "summary": "Delete an event",
        "description": "Permanently deletes an event. First call without confirmed to get a preview.",
        "operationId": "deleteEvent",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "eventId"
                ],
                "properties": {
                  "confirmed": {
                    "type": "boolean",
                    "description": "Set to true only after user confirms"
                  },
                  "eventId": {
                    "type": "string",
                    "description": "ID of the event to delete"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Event deleted or preview returned",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    },
    "/get-bookings": {
      "post": {
        "summary": "Get bookings with optional filtering",
        "description": "Retrieves bookings. Can filter by user, child, date, or date range. Provide bookingId for single booking details.",
        "operationId": "getBookings",
        "requestBody": {
          "required": false,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "bookingId": {
                    "type": "string",
                    "description": "Get a specific booking by ID"
                  },
                  "userId": {
                    "type": "string",
                    "description": "Filter by user ID"
                  },
                  "childId": {
                    "type": "string",
                    "description": "Filter by child ID"
                  },
                  "date": {
                    "type": "string",
                    "description": "Filter by specific date (YYYY-MM-DD)"
                  },
                  "fromDate": {
                    "type": "string",
                    "description": "Start of date range (YYYY-MM-DD)"
                  },
                  "toDate": {
                    "type": "string",
                    "description": "End of date range (YYYY-MM-DD)"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "List of bookings or single booking",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    },
    "/create-booking": {
      "post": {
        "summary": "Create a new booking",
        "description": "Creates a new booking/appointment. First call without confirmed to get a preview. Always pass both userId and userEmail from the system context.",
        "operationId": "createBooking",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "name",
                  "date",
                  "time"
                ],
                "properties": {
                  "confirmed": {
                    "type": "boolean",
                    "description": "Set to true only after user confirms"
                  },
                  "name": {
                    "type": "string",
                    "description": "Booking name/title"
                  },
                  "date": {
                    "type": "string",
                    "description": "Booking date (YYYY-MM-DD)"
                  },
                  "time": {
                    "type": "string",
                    "description": "Booking time (HH:MM format)"
                  },
                  "notes": {
                    "type": "string",
                    "description": "Additional notes"
                  },
                  "userId": {
                    "type": "string",
                    "description": "ID of the user from system context"
                  },
                  "userEmail": {
                    "type": "string",
                    "description": "Email of the user from system context (used as fallback)"
                  },
                  "childId": {
                    "type": "string",
                    "description": "Optional: ID of the child this booking is for"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Booking created or preview returned",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    },
    "/delete-booking": {
      "post": {
        "summary": "Delete a booking",
        "description": "Permanently deletes a booking. First call without confirmed to get a preview.",
        "operationId": "deleteBooking",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "bookingId"
                ],
                "properties": {
                  "confirmed": {
                    "type": "boolean",
                    "description": "Set to true only after user confirms"
                  },
                  "bookingId": {
                    "type": "string",
                    "description": "ID of the booking to delete"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Booking deleted or preview returned",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    },
    "/get-meals": {
      "post": {
        "summary": "Get meals with optional filtering",
        "description": "Retrieves meals from the database. Can filter by user, category, or search for templates.",
        "operationId": "getMeals",
        "requestBody": {
          "required": false,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "mealId": {
                    "type": "string",
                    "description": "Get a specific meal by ID"
                  },
                  "userId": {
                    "type": "string",
                    "description": "Filter by user ID"
                  },
                  "category": {
                    "type": "string",
                    "description": "Filter by meal category (breakfast, lunch, dinner, snack)"
                  },
                  "isTemplate": {
                    "type": "boolean",
                    "description": "Filter by template status"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "List of meals or single meal",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "meals": {
                      "type": "array",
                      "items": {
                        "type": "object"
                      }
                    },
                    "count": {
                      "type": "integer"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/create-meal": {
      "post": {
        "summary": "Create a new meal",
        "description": "Creates a new meal in the database. First call without confirmed to get a preview. Always pass both userId and userEmail from the system context.",
        "operationId": "createMeal",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "name",
                  "category"
                ],
                "properties": {
                  "confirmed": {
                    "type": "boolean",
                    "description": "Set to true only after user confirms"
                  },
                  "name": {
                    "type": "string",
                    "description": "Meal name/title"
                  },
                  "description": {
                    "type": "string",
                    "description": "Meal description"
                  },
                  "ingredients": {
                    "type": "string",
                    "description": "JSON array of ingredients"
                  },
                  "category": {
                    "type": "string",
                    "description": "Meal category: breakfast, lunch, dinner, or snack"
                  },
                  "isTemplate": {
                    "type": "boolean",
                    "description": "Whether this is a template meal"
                  },
                  "allergyInfo": {
                    "type": "string",
                    "description": "JSON array of allergy information"
                  },
                  "prepTime": {
                    "type": "integer",
                    "description": "Preparation time in minutes"
                  },
                  "userId": {
                    "type": "string",
                    "description": "ID of the user from system context"
                  },
                  "userEmail": {
                    "type": "string",
                    "description": "Email of the user from system context (used as fallback)"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Meal created or preview returned",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "meal": {
                      "type": "object"
                    },
                    "message": {
                      "type": "string"
                    },
                    "requiresConfirmation": {
                      "type": "boolean"
                    },
                    "preview": {
                      "type": "object"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/update-meal": {
      "post": {
        "summary": "Update an existing meal",
        "description": "Updates a meal's information. First call without confirmed to get a preview.",
        "operationId": "updateMeal",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "mealId"
                ],
                "properties": {
                  "confirmed": {
                    "type": "boolean",
                    "description": "Set to true only after user confirms"
                  },
                  "mealId": {
                    "type": "string",
                    "description": "ID of the meal to update"
                  },
                  "name": {
                    "type": "string"
                  },
                  "description": {
                    "type": "string"
                  },
                  "ingredients": {
                    "type": "string"
                  },
                  "category": {
                    "type": "string"
                  },
                  "isTemplate": {
                    "type": "boolean"
                  },
                  "allergyInfo": {
                    "type": "string"
                  },
                  "prepTime": {
                    "type": "integer"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Meal updated or preview returned",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    },
    "/delete-meal": {
      "post": {
        "summary": "Delete a meal",
        "description": "Permanently deletes a meal. First call without confirmed to get a preview.",
        "operationId": "deleteMeal",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "mealId"
                ],
                "properties": {
                  "confirmed": {
                    "type": "boolean",
                    "description": "Set to true only after user confirms"
                  },
                  "mealId": {
                    "type": "string",
                    "description": "ID of the meal to delete"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Meal deleted or preview returned",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    },
    "/get-meal-plans": {
      "post": {
        "summary": "Get meal plans with optional filtering",
        "description": "Retrieves meal plans from the database. Can filter by user, active status, or date range.",
        "operationId": "getMealPlans",
        "requestBody": {
          "required": false,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "mealPlanId": {
                    "type": "string",
                    "description": "Get a specific meal plan by ID"
                  },
                  "userId": {
                    "type": "string",
                    "description": "Filter by user ID"
                  },
                  "isActive": {
                    "type": "boolean",
                    "description": "Filter by active status"
                  },
                  "includeEntries": {
                    "type": "boolean",
                    "description": "Include meal plan entries with details"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "List of meal plans or single meal plan",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "mealPlans": {
                      "type": "array",
                      "items": {
                        "type": "object"
                      }
                    },
                    "count": {
                      "type": "integer"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/create-meal-plan": {
      "post": {
        "summary": "Create a new meal plan for the week",
        "description": "Creates a new meal plan with start and end dates. First call without confirmed to get a preview. Always pass both userId and userEmail from the system context.",
        "operationId": "createMealPlan",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "name",
                  "startDate",
                  "endDate"
                ],
                "properties": {
                  "confirmed": {
                    "type": "boolean",
                    "description": "Set to true only after user confirms"
                  },
                  "name": {
                    "type": "string",
                    "description": "Meal plan name (e.g., 'Week of Jan 15')"
                  },
                  "startDate": {
                    "type": "string",
                    "description": "Start date of the meal plan (YYYY-MM-DD), typically Monday"
                  },
                  "endDate": {
                    "type": "string",
                    "description": "End date of the meal plan (YYYY-MM-DD), typically Sunday"
                  },
                  "isActive": {
                    "type": "boolean",
                    "description": "Whether the meal plan is active"
                  },
                  "userId": {
                    "type": "string",
                    "description": "ID of the user from system context"
                  },
                  "userEmail": {
                    "type": "string",
                    "description": "Email of the user from system context (used as fallback)"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Meal plan created or preview returned",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "mealPlan": {
                      "type": "object"
                    },
                    "message": {
                      "type": "string"
                    },
                    "requiresConfirmation": {
                      "type": "boolean"
                    },
                    "preview": {
                      "type": "object"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/add-meal-to-plan": {
      "post": {
        "summary": "Add a meal to a meal plan",
        "description": "Adds a specific meal to a meal plan for a particular day and meal time. First call without confirmed to get a preview.",
        "operationId": "addMealToPlan",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "mealPlanId",
                  "mealId",
                  "dayOfWeek",
                  "mealTime"
                ],
                "properties": {
                  "confirmed": {
                    "type": "boolean",
                    "description": "Set to true only after user confirms"
                  },
                  "mealPlanId": {
                    "type": "string",
                    "description": "ID of the meal plan"
                  },
                  "mealId": {
                    "type": "string",
                    "description": "ID of the meal to add"
                  },
                  "dayOfWeek": {
                    "type": "integer",
                    "description": "Day of week (0=Monday, 1=Tuesday, ... 6=Sunday)"
                  },
                  "mealTime": {
                    "type": "string",
                    "description": "Meal time: breakfast, lunch, dinner, or snack"
                  },
                  "notes": {
                    "type": "string",
                    "description": "Optional notes for this meal entry"
                  }
                }
              }
            }
          }
        },
